//Where is the LED connected
//Port: A
//Pin:  5
#define PERIPH_BASE  					(0x4000 0000UL) // UL-Unsigned Long Constant
#define AHB1PERIPH_OFFSET 				(0x0002 0000UL)
#define AHB1PERIPH_BASE                 (PERIPH_BASE + AHB1PERIPH_OFFSET)
#define GPIOA_OFFSET					(0x0000U)


#define GPIOA_BASE                      (AHB1PERIPH_BASE + GPIOA_OFFSET)

#define RCC_OFFSET						(0x3800UL)
#define RCC_BASE						(AHB1PERIPH_BASE + RCC_OFFSET)

#define AHB1EN_R_OFFSET                 ( 0x30UL)
#define RCC_AHB1EN_R					(RCC_BASE + AHB1EN_R_OFFSET)

#define MODER_OFFSET					(0x00UL)
#define GPIOA_MODER						(GPIOA_OFFSET + MODER_OFFSET)



#define GPIOAEN							(1U<<0) // 0(0b): 0000 0000 0000 0000
												//1U <<0 : 0000 0000 0000 0001
												//1U<<1 : 0000 0000 0000 0010
												//1U<<2 : 0000 0000 0000 0100
